<Window x:Class="RCP_Klient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RCP_Klient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Button_Login" Content="Login" HorizontalAlignment="Left" Margin="13,67,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Label x:Name="Label_Login" Content="Login:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Label_Password" Content="Hasło:" HorizontalAlignment="Left" Margin="9,36,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBox_Login" HorizontalAlignment="Left" Margin="57,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="PasswordBox_Password" HorizontalAlignment="Left" Margin="57,40,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
